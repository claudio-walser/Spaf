<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>Simple Php Application Framework</title>
	<parser>
		<target>./docs/</target>
	</parser>
	<transformer>
		<target>./docs/</target>
	</transformer>
	<transformations>
		<template name="responsive-twig" />
		<!--
			Available templates:
			* responsive
			* responsive-twig
			* new-black
			* abstract
			* zend
			* old-ocean
			* checkstyle
		-->		
	</transformations>
	<files>
		<directory>./src/</directory>
		<!--<ignore>tests/Data/</ignore>
		<ignore>_tests/*</ignore>
		<ignore>_tools/*</ignore>-->
	</files>
</phpdoc>